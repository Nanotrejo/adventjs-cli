  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug: init",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/index.js",
      "preLaunchTask": "tsc: build index.ts",
      "args": ["init"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "name": "Debug: init DEV",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/index.js",
      "preLaunchTask": "tsc: build index.ts (DEV)",
      "args": ["init"],
      "env": {
        "DEV": "true"
      },
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "name": "Debug: g <day> ts",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/index.js",
      "preLaunchTask": "tsc: build index.ts",
      "args": ["g", "1", "ts"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "name": "Debug: g <day> <py>",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/index.js",
      "preLaunchTask": "tsc: build index.ts",
      "args": ["g", "1", "py"],
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "name": "Debug: g <day> <ts> DEV",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/dist/index.js",
      "preLaunchTask": "tsc: build index.ts (DEV)",
      "args": ["g", "1","ts"],
      "env": {
        "DEV": "true"
      },
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    }
  ]
}
